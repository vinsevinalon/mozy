{
  "wrapper": "div.collection-wrapper",
  "sections": {
    "collection_header": {
      "type": "template--collection-header",
      "blocks": {
        "collection_title": {
          "type": "collection-title",
          "settings": {
          }
        },
        "collection_description": {
          "type": "collection-description",
          "settings": {
          }
        }
      },
      "block_order": [
        "collection_title",
        "collection_description"
      ],
      "settings": {
        "collection_image_enable": false
      }
    },
    "product_filtering": {
      "type": "main-collection-filtering",
      "settings": {
        "desktop_position": "sidebar",
        "enable_filtering": false,
        "enable_sorting": false
      }
    },
    "main": {
      "type": "template--collection",
      "blocks": {
        "product_title": {
          "type": "product-card-title",
          "settings": {
          }
        },
        "product_price": {
          "type": "product-card-price",
          "settings": {
          }
        }
      },
      "block_order": [
        "product_title",
        "product_price"
      ],
      "settings": {
        "grid": 3,
        "grid_mobile": "per_row_two",
        "rows": 6,
        "pagination_type": "load_more"
      }
    }
  },
  "order": [
    "collection_header",
    "product_filtering",
    "main"
  ]
}
